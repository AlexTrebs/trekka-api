{
  "version": 2,
  "builds": [
    {
      "src": "api/index.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.go",
      "headers": {
        "Cache-Control": "public, s-maxage=900"
      }
    }
  ],
  "functions": {
    "api/index.go": {
      "maxDuration": 10,
      "memory": 1024
    }
  }
}
